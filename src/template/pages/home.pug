									path(d='M30.9977 17.8688L24.7368 11.608C24.3298 11.201 23.6682 11.201 23.2613 11.608C22.8543 12.0149 22.8543 12.6765 23.2613 13.0835L29.5221 19.3443C29.7267 19.5467 29.9938 19.6491 30.2609 19.6491C30.5281 19.6491 30.7952 19.5468 30.9977 19.3443C31.4046 18.9373 31.4046 18.2758 30.9977 17.8688Z')
								p Скидка 20% активирована
							+b.A.prices__item-btn.btn._white-hov(href='#!')
								span ПРОЙТИ ПРОГРАММУ

					+e.item(data-anim-stagger='fade-y')
						+e.item-value
							h5
								sup €
								| 35.32
						+e.item-value._uah
							h5 (900.99 грн)
						+e.item-title
							h5
								span WOWBODY STRONG + STRONG 2
						+checkBox('__item-checkbox', '8 недель')(name='price-checkbox' type='radio' checked)
						+b.promo-code.__item-promo-code(data-anim-stagger="fade-y")
							+e.BUTTON.action-btn.js-promo-code-enter-btn
								span ВВЕСТИ ПРОМОКОД
								svg(class='promo-code__action-btn-icon' version='1.1', xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink', x='0px', y='0px', viewBox='-90.175 170.214 792.175 451.746')
									path(d='M266.733 605.657L-73.872 265.052c-21.737-21.737-21.737-56.797 0-78.534 21.736-21.737 56.796-21.737 78.358 0l301.338 301.34 301.514-301.164c21.562-21.737 56.796-21.737 78.358 0 21.736 21.562 21.736 56.797 0 78.358L345.092 605.657c-10.87 10.868-25.068 16.303-39.267 16.303-14.024 0-28.223-5.435-39.092-16.303z')
							+e.FORM.form.form(action='#!' method='POST')
								+e.form-group.form-group.js-form-group-clear
