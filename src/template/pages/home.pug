									path(d='M16.8417 1.3421C15.6208 0.978975 14.2622 1.45059 13.4879 2.48784C12.8994 3.27877 12.722 4.26172 12.9975 5.18627C14.4333 10.0155 18.9558 13.388 23.9999 13.388C24.5759 13.388 25.0434 12.9205 25.0434 12.3445C25.0434 7.30244 21.6709 2.7779 16.8417 1.3421ZM14.9968 4.59149C14.8799 4.19706 15.0489 3.88607 15.1616 3.73581C15.364 3.46454 15.6708 3.30175 15.9797 3.30175C16.0694 3.30175 16.1592 3.31633 16.2469 3.34353C19.8406 4.40993 22.4472 7.56961 22.8897 11.2343C19.2229 10.7897 16.0653 8.18317 14.9968 4.59149Z')
									path(d='M34.512 2.48977C33.7399 1.45253 32.3729 0.980907 31.1583 1.34198C26.3291 2.77777 22.9565 7.30027 22.9565 12.3444C22.9565 12.9204 23.4241 13.3879 24.0001 13.3879C29.0443 13.3879 33.5667 10.0153 35.0025 5.1882C35.2801 4.26375 35.1006 3.27864 34.512 2.48977ZM33.0011 4.59137C31.9347 8.1851 28.775 10.7917 25.1104 11.2341C25.5549 7.56733 28.1615 4.4098 31.7532 3.34125C31.8429 3.31621 31.9327 3.30163 32.0203 3.30163C32.3292 3.30163 32.6359 3.46441 32.8363 3.73569C32.949 3.88595 33.1201 4.19693 33.0011 4.59137Z')
